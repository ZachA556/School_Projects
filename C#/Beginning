
using System;
public class Module_1
{
    public static void Main()
    {
        Console.Write("Enter a number: ");
        int num1 = Convert.ToInt32(Console.ReadLine());

        
        int num2 = 100;
        int num3 = 2;

        
        Console.WriteLine(  num1 * num1);
        Console.WriteLine(  num1 / num3);
        Console.WriteLine(num1 - num2);

    }
}
